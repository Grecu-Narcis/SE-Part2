<Window x:Class="Frontend.PaymentsAndBillings.PaymentsAndBillingsMain"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Frontend.PaymentsAndBillings"
        mc:Ignorable="d"
        Title="PaymentsAndBillingsMain" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Button Content="Bank Account Details" Click="BankAccountDetails_Click" HorizontalAlignment="Left" Width="200" Margin="5,10,0,0"/>
            <Button Content="Payment Form" Click="PaymentForm_Click" HorizontalAlignment="Left" Width="200" Margin="5,10,0,0"/>
        </StackPanel>
    </Grid>
</Window>
